        "macIconPath": "/project-folder.icns",
        "newProjectDir": "/projects/new",
        "defaultProjectDir": "/projects/default",
        "errorProjectDir": "/projects/error",
        "settingsFileName": "ad.json",
        "assetsDirName": "assets",
        "supportedAssets": ["png", "jpg", "jpeg", "gif", "webp", "tif", "tiff", "bmp", "svg", "mp4", "ogg", "wav", "mp3", "jsv", "js", "html", "css", "json", "xsl", "xslx", "csv", "pdf"],
        "componentDirNamePrefix": "",
        "css": {
            "fileName": "index.css",
            "templateFilePath": "/css/base.css",
            "globalFileName": "global-styles.css",
            "syntax": {
                "contexts": ["time", "environment", "location", "user", "input", "device", "site", "format", "layout", "custom", "panel"],
                "contextOperators": [":", ">=", "<=", "=="],
                "attrPrefix": "[data-",
                "attrJoiner": "~=\"",
                "attrExplicitJoiner": "=\"",
                "attrEnding": "\"]",
                "flowlane": "flowlane",
                "flowlaneProperties": ["min-width", "max-width", "min-scale", "max-scale"],
                "flowlaneDefaults": {
                    "breakpoints": [],
                    "min-width": 0,
                    "max-width": 10000,
                    "min-scale": 0,
                    "max-scale": 1000
                },
                "flowlaneDefine": "define-flowlane",
                "breakpoint": "breakpoint",
                "defaultBreakpointType": "max"
            },
            "normalizeDir": "/css/normalize",
            "normalize": [{
                "tag": "div",
                "css": "a.css"
            }]
        },
        "html": {
            "fileName": "index.html",
            "templateFilePath": "/templates/component.html",
            "syntax": {
                "prefix": "ad-",
                "attributeFinalPrefix": "data-"
            }
        },
        "version": "1.0.12",
        "useUserPluginsFolder": false
    }
